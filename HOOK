local eheyl = main:section("俄亥俄州[娱乐功能]", false)
-- 加载UI库
-- 添加文本编辑框（用于输入嘲讽内容）
eheyl:Textbox(
    "输入嘲讽内容",          -- 文本框标签
    "taunt_message",     -- 唯一标识符（flag）
    "Snow NO1",       -- 默认文本
    function(text)       -- 回调函数（当内容改变时触发）
        print("嘲讽内容已更新:", text)
        _G.TauntMessage = text -- 存储到全局变量
    end
)

-- 添加嘲讽开关（开启后自动循环发送，关闭停止）
local tauntEnabled = false
local tauntInterval = 2 -- 发送间隔(秒)

eheyl:Toggle("嘲讽开关", "", false, function(state)
    tauntEnabled = state
    if state then
        -- 开启时启动循环发送
        spawn(function()
            while tauntEnabled do
                if _G.TauntMessage then
                    local TextChatService = game:GetService("TextChatService")
                    local chatSystem = TextChatService.TextChannels.RBXGeneral
                    if chatSystem then
                        chatSystem:SendAsync(_G.TauntMessage)
                    end
                else
                    warn("请先输入嘲讽内容！")
                    tauntEnabled = false
                    -- 这里可以添加代码自动关闭开关
                    break
                end
                wait(tauntInterval) -- 等待指定间隔
            end
        end)
    else

    end
end)

-- 添加间隔时间调整滑块（可选）
eheyl:Slider("嘲讽间隔", "taunt_interval", 1, 1, 10, false, function(value)
    tauntInterval = value
end)
