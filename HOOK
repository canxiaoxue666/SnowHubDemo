--头文件以及其他
local Player = game:GetService("Players").LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

-- 获取目标文件夹
local goldFolder = workspace:FindFirstChild("Game")
                  and workspace.Game:FindFirstChild("Entities")
                  and workspace.Game.Entities:FindFirstChild("GoldJewelSafe")

local jewelFolder = workspace:FindFirstChild("Game")
                  and workspace.Game:FindFirstChild("Entities")
                  and workspace.Game.Entities:FindFirstChild("JewelSafe")

-- 存储UI和状态
local screenGui
local statsFrame
local modelTextMap = {}
local goldCount = 0
local blackCount = 0
local isEnabled = false

-- 文本标签参数
local FONT_SIZE = 24
local TEXT_WIDTH = 120
local TEXT_HEIGHT = 40
local TEXT_OFFSET = Vector3.new(0, 2, 0)

-- 创建UI元素
local function createUI()
    if screenGui and screenGui.Parent then return end
    
    screenGui = Instance.new("ScreenGui")
    screenGui.Name = "JewelDisplay"
    screenGui.ResetOnSpawn = false
    screenGui.Parent = PlayerGui

    -- 创建左上角统计窗口
    statsFrame = Instance.new("Frame")
    statsFrame.Name = "InsuranceStats"
    statsFrame.Size = UDim2.new(0, 180, 0, 80)
    statsFrame.Position = UDim2.new(0, 10, 0, 10)
    statsFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    statsFrame.BackgroundTransparency = 0.5
    statsFrame.BorderSizePixel = 0
    statsFrame.Parent = screenGui

    local titleLabel = Instance.new("TextLabel")
    titleLabel.Name = "Title"
    titleLabel.Size = UDim2.new(1, 0, 0, 25)
    titleLabel.Position = UDim2.new(0, 0, 0, 0)
    titleLabel.Text = "保险统计"
    titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    titleLabel.BackgroundTransparency = 1
    titleLabel.Font = Enum.Font.SourceSansBold
    titleLabel.TextSize = 18
    titleLabel.Parent = statsFrame

    local goldCountLabel = Instance.new("TextLabel")
    goldCountLabel.Name = "GoldCount"
    goldCountLabel.Size = UDim2.new(1, 0, 0, 25)
    goldCountLabel.Position = UDim2.new(0, 0, 0, 25)
    goldCountLabel.Text = "金保险: 0"
    goldCountLabel.TextColor3 = Color3.fromRGB(255, 200, 0)
    goldCountLabel.BackgroundTransparency = 1
    goldCountLabel.Font = Enum.Font.SourceSans
    goldCountLabel.TextSize = 16
    goldCountLabel.TextXAlignment = Enum.TextXAlignment.Left
    goldCountLabel.Parent = statsFrame

    local blackCountLabel = Instance.new("TextLabel")
    blackCountLabel.Name = "BlackCount"
    blackCountLabel.Size = UDim2.new(1, 0, 0, 25)
    blackCountLabel.Position = UDim2.new(0, 0, 0, 50)
    blackCountLabel.Text = "黑保险: 0"
    blackCountLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
    blackCountLabel.BackgroundTransparency = 1
    blackCountLabel.Font = Enum.Font.SourceSans
    blackCountLabel.TextSize = 16
    blackCountLabel.TextXAlignment = Enum.TextXAlignment.Left
    blackCountLabel.Parent = statsFrame
end

-- 销毁UI元素
local function destroyUI()
    if screenGui then
        screenGui:Destroy()
        screenGui = nil
        statsFrame = nil
    end
    
    -- 清除所有文本标签
    for model, data in pairs(modelTextMap) do
        if data and data.label then
            data.label:Destroy()
        end
    end
    modelTextMap = {}
    goldCount = 0
    blackCount = 0
end

-- 更新统计显示
local function updateStatsDisplay()
    if not statsFrame then return end
    
    local goldLabel = statsFrame:FindFirstChild("GoldCount")
    local blackLabel = statsFrame:FindFirstChild("BlackCount")
    
    if goldLabel then goldLabel.Text = "金保险: "..goldCount end
    if blackLabel then blackLabel.Text = "黑保险: "..blackCount end
end

-- 检查是否是Door模型
local function isDoorModel(model)
    return model.Name == "Door"
end

-- 创建/更新单个Model的文本标签
local function updateModelText(model)
    if not isEnabled then return end
    
    -- 跳过Door模型
    if isDoorModel(model) then
        if modelTextMap[model] then
            if modelTextMap[model].isGold then
                goldCount = goldCount - 1
            else
                blackCount = blackCount - 1
            end
            modelTextMap[model].label:Destroy()
            modelTextMap[model] = nil
            updateStatsDisplay()
        end
        return
    end

    -- 确定模型属于哪个文件夹
    local isGold = goldFolder and model:IsDescendantOf(goldFolder)
    local isBlack = jewelFolder and model:IsDescendantOf(jewelFolder)
    
    if not isGold and not isBlack then return end

    -- 设置文本和颜色
    local text, textColor, strokeColor
    if isGold then
        text = "金保险"
        textColor = Color3.fromRGB(255, 200, 0)
        strokeColor = Color3.fromRGB(100, 80, 0)
    else
        text = "黑保险"
        textColor = Color3.fromRGB(0, 0, 0)
        strokeColor = Color3.fromRGB(50, 50, 50)
    end

    -- 获取或创建文本标签
    local data = modelTextMap[model]
    
    if not data and model:IsA("Model") then
        local textLabel = Instance.new("TextLabel")
        textLabel.Name = (isGold and "GoldLabel_" or "BlackLabel_")..model.Name
        textLabel.Size = UDim2.new(0, TEXT_WIDTH, 0, TEXT_HEIGHT)
        textLabel.Text = text
        textLabel.TextSize = FONT_SIZE
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.TextColor3 = textColor
        textLabel.BackgroundTransparency = 1
        textLabel.TextStrokeTransparency = 0.5
        textLabel.TextStrokeColor3 = strokeColor
        textLabel.Parent = screenGui
        
        data = {
            label = textLabel,
            isGold = isGold
        }
        modelTextMap[model] = data
        
        -- 更新计数
        if isGold then
            goldCount = goldCount + 1
        else
            blackCount = blackCount + 1
        end
        updateStatsDisplay()
    elseif data then
        -- 更新现有标签
        data.label.Text = text
        data.label.TextColor3 = textColor
        data.label.TextStrokeColor3 = strokeColor
    end

    -- 更新位置
    if data then
        local part = model:FindFirstChildOfClass("BasePart") or model.PrimaryPart
        if part then
            local position, onScreen = workspace.CurrentCamera:WorldToScreenPoint(part.Position + TEXT_OFFSET)
            if onScreen then
                data.label.Position = UDim2.new(0, position.X - TEXT_WIDTH/2, 0, position.Y - TEXT_HEIGHT/2)
                data.label.Visible = true
                return
            end
        end
        data.label.Visible = false
    end
end

-- 清理无效文本标签
local function cleanupTextLabels()
    for model, data in pairs(modelTextMap) do
        if not model.Parent or not data.label.Parent then
            if data.isGold then
                goldCount = goldCount - 1
            else
                blackCount = blackCount - 1
            end
            data.label:Destroy()
            modelTextMap[model] = nil
            updateStatsDisplay()
        end
    end
end

-- 初始化现有Model
local function initializeExistingModels()
    if goldFolder then
        for _, model in ipairs(goldFolder:GetDescendants()) do
            if model:IsA("Model") then
                updateModelText(model)
            end
        end
    end
    
    if jewelFolder then
        for _, model in ipairs(jewelFolder:GetDescendants()) do
            if model:IsA("Model") then
                updateModelText(model)
            end
        end
    end
end

-- 监听Model变化
local function setupFolderListeners(folder)
    if not folder then return end
    
    folder.DescendantAdded:Connect(function(descendant)
        if descendant:IsA("Model") then
            task.wait(0.5)
            updateModelText(descendant)
        end
    end)

    folder.DescendantRemoving:Connect(function(descendant)
        if modelTextMap[descendant] then
            if modelTextMap[descendant].isGold then
                goldCount = goldCount - 1
            else
                blackCount = blackCount - 1
            end
            modelTextMap[descendant].label:Destroy()
            modelTextMap[descendant] = nil
            updateStatsDisplay()
        end
    end)
end

-- 开关控制
local function toggleJewelDisplay(value)
    isEnabled = value
    
    if value then
        createUI()
        initializeExistingModels()
        setupFolderListeners(goldFolder)
        setupFolderListeners(jewelFolder)
        
        -- 启动每帧更新
        game:GetService("RunService").RenderStepped:Connect(function()
            if not isEnabled then return end
            cleanupTextLabels()
            for model in pairs(modelTextMap) do
                updateModelText(model)
            end
        end)
    else
        destroyUI()
    end
end
--黑市
-- 黑市互动功能
     hs = function()
    while hshd and task.wait() do
        local prompt = game:GetService("Workspace")
            :FindFirstChild("BlackMarket")
            and game.Workspace.BlackMarket:FindFirstChild("Dealer")
            and game.Workspace.BlackMarket.Dealer:FindFirstChild("Dealer")
            and game.Workspace.BlackMarket.Dealer.Dealer:FindFirstChildOfClass("ProximityPrompt")
        
        if prompt then
            prompt.HoldDuration = 0
            prompt.MaxActivationDistance = 99999
            fireproximityprompt(game:GetService("Workspace").BlackMarket.Dealer.Dealer.ProximityPrompt)
        end
    end
end
jshd = function()
local function modifyPrompt(prompt)
    prompt.HoldDuration = 0
end

-- 检查 ProximityPrompt 是否在目标路径下
local function isTargetPrompt(prompt)
    local parent = prompt.Parent
    while parent do
        if parent == workspace.Game.Entities or parent == workspace.BankRobbery.VaultDoor then
            return true
        end
        parent = parent.Parent
    end
    return false
end

-- 修改现有的目标 ProximityPrompt
for _, prompt in ipairs(workspace:GetDescendants()) do
    if prompt:IsA("ProximityPrompt") and isTargetPrompt(prompt) then
        modifyPrompt(prompt)
    end
end

-- 监听新生成的 ProximityPrompt
workspace.DescendantAdded:Connect(function(instance)
    if instance:IsA("ProximityPrompt") and isTargetPrompt(instance) then
        modifyPrompt(instance)
    end
end)
end
