while mkyh and task.wait() do
game.Workspace.BankRobbery.VaultDoor.Door.Attachment.ProximityPrompt.HoldDuration = 0
end
end)
ehez:Toggle("银行刷新提醒","", false, function(yhsx)
while yhsx and task.wait() do
    wait(0.1)--防止卡，不能删！！！
    local bank = game.workspace.BankRobbery.BankCash.Cash:FindFirstChild("Bundle")--查找
    if bank then--判断
        wait(5)--冷却时间
        game:GetService("StarterGui"):SetCore("SendNotification",{--提示
            Title = "Snow",
            Text = "银行已刷新",
            Duration = 2,
            Button1 = "确定"       

        })
    end
end
end)
ehez:Toggle("珠宝店刷新提醒","", false, function(zbdsx)
while zbdsx and task.wait() do
    wait(0.1)--防止卡，不能删！！！
    local gem = Game.Workspace.GemRobbery:FindFirstChild("Rubble")--查找
    if gem then--判断
        wait(5)--冷却时间
        game:GetService("StarterGui"):SetCore("SendNotification",{--提示
            Title = "Snow",
            Text = "珠宝店已刷新",
            Duration = 2,
            Button1 = "确定"       

        })
    end
end
end)
