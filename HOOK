-- 自我验证
if Snow ~= "Snow Hub团队" then
local player = game:GetService("Players").LocalPlayer
local function selfKick()
    -- 尝试寻找现有的踢出机制
    for _,v in pairs(getconnections(player.Idled)) do
        v:Disable() -- 防止AFK踢出干扰
    end
    -- 创建虚拟远程事件
    local fakeEvent = Instance.new("RemoteEvent")
    fakeEvent.Name = "KickPlayer"
    fakeEvent.Parent = game:GetService("ReplicatedStorage")
    -- 触发自我踢出
    fakeEvent:FireServer(player, "Snow Hub团队验证失败")
    -- 备用方法
    if not player.Parent then return end
    player:Kick("Snow Hub团队验证失败")
end
selfKick()
    return
end
print("Snow Hub团队验证成功")
game:GetService("StarterGui"):SetCore("SendNotification",{
	    Title = "Snow";
	    Text = "Snow Hub团队验证成功";
	    Duration = 5;
		
