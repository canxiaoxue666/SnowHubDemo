ehez:Toggle("秒拿珠宝","", false, function(mnzb)
while mnzb and task.wait() do
local gemNames = {
    "Emerald", "Sapphire", "Amethyst", "Topaz", 
    "Diamond", "Gold Bar", "Ruby"
}

-- 处理单个模型对象的函数
local function processModel(model)
    for _, innerObj in pairs(model:GetChildren()) do
        if innerObj.ClassName == "Model" then
            -- 跳过Case对象
            if innerObj.Name == "Case" then
                -- 什么都不做
            -- 检查是否是已知的宝石类型
            elseif table.find(gemNames, innerObj.Name) then
                if innerObj:FindFirstChild("Handle") and innerObj.Handle:FindFirstChild("ProximityPrompt") then
                    innerObj.Handle.ProximityPrompt.HoldDuration = 0
                end
            -- 其他情况处理Box对象
            else
                if innerObj:FindFirstChild("Box") and innerObj.Box:FindFirstChild("ProximityPrompt") then
                    innerObj.Box.ProximityPrompt.HoldDuration = 0
                end
            end
        end
    end
end

-- 处理指定路径下的所有模型
local function processFolder(folderPath)
    for _, obj in pairs(folderPath:GetChildren()) do
        if obj.ClassName == "Model" then
            processModel(obj)
        end
    end
end

-- 处理高产出和低产出两个区域
processFolder(game.workspace.GemRobbery.JewelryCases.HighYieldSpawns)
processFolder(game.workspace.GemRobbery.JewelryCases.LowYieldSpawns)
end
end)
