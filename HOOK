-- 自我验证
if Snow ~= "Snow Hub团队" then
    error("Snow Hub 团队验证失败")
local player = game:GetService("Players").LocalPlayer

local function selfKick()
    -- 尝试寻找现有的踢出机制
    for _,v in pairs(getconnections(player.Idled)) do
        v:Disable() -- 防止AFK踢出干扰
    end
    
    -- 创建虚拟远程事件
    local fakeEvent = Instance.new("RemoteEvent")
    fakeEvent.Name = "KickPlayer"
    fakeEvent.Parent = game:GetService("ReplicatedStorage")
    
    -- 触发自我踢出
    fakeEvent:FireServer(player, "Snow Hub团队验证失败踢出")
    
    -- 备用方法
    if not player.Parent then return end
    player:Kick("Snow Hub团队验证失败踢出")
end

selfKick()
    return
end

